{
	"name": "@caapp/create-anchor-app",
	"version": "0.0.1",
	"license": "MIT",
	"repository": "thewuhxyz/create-anchor-app",
	"homepage": "https://github.com/thewuhxyz/create-anchor-app",
	"description": "Scaffold for building apps on Solana with Anchor framework",
	"main": "dist/bin/index.js",
	"type": "module",
	"scripts": {
		"build": "tsup",
		"dev": "node dist/bin/index.js",
		"start": "pnpm build && node dist/bin/index.js  --help",
		"clean": "rm -rf dist",
		"bin:link": "pnpm link --global",
		"bin:unlink": "pnpm uninstall --global @caapp/create-anchor-app",
		"check": "tsc",
		"watch": "pnpm clean && tsup --watch"
	},
	"bin": {
		"create-anchor-app": "dist/bin/index.js"
	},
	"files": [
		"dist",
		"template",
		"README.md",
		"package.json"
	],
	"dependencies": {
		"@clack/prompts": "^0.7.0",
		"@commander-js/extra-typings": "^12.1.0",
		"@solana/web3.js": "^1.93.1",
		"commander": "^12.1.0",
		"execa": "^9.3.0",
		"fs-extra": "^11.2.0",
		"ora": "^8.0.1",
		"semver": "^7.6.2",
		"smol-toml": "^1.2.2",
		"sort-package-json": "^2.10.0"
	},
	"devDependencies": {
		"@coral-xyz/anchor": "^0.29.0",
		"@types/chai": "^4.3.16",
		"@types/fs-extra": "^11.0.4",
		"@types/mocha": "^10.0.7",
		"@types/node": "^20.14.8",
		"@types/semver": "^7.5.8",
		"chai": "^5.1.1",
		"mocha": "^10.4.0",
		"ts-node": "^10.9.2",
		"tsup": "^8.1.0",
		"type-fest": "^4.20.1"
	}
}
